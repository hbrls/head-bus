{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "forge": true
  },
  "rules": {
    "func-names": 0,
    "max-len": 0,
    "no-else-return": 0,
    "no-underscore-dangle": 0,
    "prefer-template": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "no-plusplus": 0,
    "prefer-rest-params": 0,
  }
}
